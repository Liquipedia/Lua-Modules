{
	"files.associations" : {
		".busted": "lua",
		".luacov": "lua"
	},
	"stylelint.validate": [
		"css",
		"postcss",
		"less",
		"scss"
	],
	"css.validate": false,
	"less.validate": false,
	"scss.validate": false,
	"workbench.editor.customLabels.patterns": {
		"**/wikis/*/*.lua": "${dirname}: ${filename}",
		"**/wikis/*/*/*.lua": "${dirname(-4)}: ${dirname}/${filename}",
		"**/wikis/*/*/*/*.lua": "${dirname(-4)}: ${dirname(-5)}/${dirname}/${filename}",
		"**/wikis/*/*/*/*/*.lua": "${dirname(-4)}: ${dirname(-5)}/${dirname(-6)}/${dirname}/${filename}",
		"**/*.lua": "${dirname}: ${filename}"
	},
	"eslint.workingDirectories": [ "./lua", "./javascript" ],
	"eslint.lintTask.enable": true,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode",
		"editor.formatOnSave": true,
	},
	"[less]": {
		"editor.defaultFormatter": "stylelint.vscode-stylelint"
	},
}
